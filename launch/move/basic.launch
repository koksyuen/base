<launch>
  <!-- Arguments -->
  <arg name="cmd_vel_topic" default="cmd_vel" />
  <arg name="odom_topic" default="odom" />

  <!-- move_base -->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">

    <rosparam file="$(find base)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find base)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find base)/param/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find base)/param/global_costmap_params.yaml" command="load" />
    
    <param name="base_local_planner" value="base_local_planner/TrajectoryPlannerROS" />

    <rosparam file="$(find base)/param/local_planner_params_basic.yaml" command="load" />    
  </node>
</launch>
